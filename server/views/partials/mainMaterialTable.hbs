 <div class="col-md-12">
    <div class="table-buttons" id="table-buttons">
        <button class="btn btn-outline-dark" id="create-new" onclick="test()"><i class="fas fa-plus"></i>Add Material</button>
    </div>
    </div>

    <div class="col-md-12">
    <table id="material-filterable-table" class="table table-bordered table-striped">
        <thead>
             <th scope="col">S.N.</th>
            <th scope="col">MR#</th>
            <th scope="col">Material</th>
            <th scope="col">Supplier</th>
            <th scope="col">Quantatiy</th>
            <th scope="col">Cost</th>
        </thead>
        <tbody>
            <tr>
                {{#each existingMain.consumables}}
                    <td>1</td>
                    <td>{{this.mr}}</td>
                    <td>{{this.material}}</td>
                    <td>{{this.supplier}}</td>
                    <td>{{this.quantity}}</td>
                    <td>{{this.cost}}</td>
                {{/each}}
            </tr>
        </tbody>
    </table>
</div>
<div id="myModal" class="modal">

  <!-- Modal content -->
  <div class="modal-content">
    <span class="close">&times;</span>
        <div id="mContent"></div>
        
  </div>

</div>