<div class="col-md-12">
	<div class="alert alert-warning" role="alert">
		<h4 class="alert-heading">Possible Issues</h4>
		{{#if error_msg}}
		<p>{{error_msg}}</p>
		{{else}}
			<p>You don't have enough permission to view this page.</p>
		{{/if}}
		<hr>
		<p class="mb-0">Please click to following button to go to dashboard.</p>
	</div>
	<div>
		<a href="/" type="button" class="btn btn-primary">Go to dashboard</a>
	</div>
</div>

<style>
	.top-flash-message
	{
		display: none;
	}
</style>