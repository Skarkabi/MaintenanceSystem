<div class="form-row">
     <div class="form-group col required">
        <label for="otherSupplierName">Supplier Name</label>
         <select type="text" id="otherSupplierName" name="otherSupplierName" class="form-control" required="required">
             {{#each values.supplier}}
                <option value="{{this.id}}">{{this.name}}</option>
             {{/each}}       
        </select>
    </div>
    <div class="form-group col required">
        <label for="oilSpec">Category</label>
         <input list="other_category"  name="other_category" class="form-control" required>
         <datalist id="other_category">
            {{#each values.other.names}}
                <option value="{{this}}"></option>
            {{/each}}
        </datalist>
    </div>
    <div class="form-group col required">
        <label for="otherMaterialRequest">Material Request Number</label>
        <input type="text" placeholder = "0" id="otherMaterialRequest" name="otherMaterialRequest" class="form-control" required>
    </div>
</div>
<div class="form-row ">
    <div class="for-group col required">
        <label for="otherPrice">Price</label>
        <input type="number" placeholder = "0" id="otherPrice" name="otherPrice" class="form-control" required>
    </div>
    <div class="form-group col required">
        <label for="quantityOther">Quantity</label>
        <input type="number" placeholder = "0" id="quantityOther" name="quantityOther" class="form-control" required>
    </div>
</div>
<div class="form-row">
    <div class="form-group col">
        <label for="otherDetails">Details</label>
        <textarea type="text" id="otherDetails" name="otherDetails" class="form-control" required></textarea>
    </div>
</div>
<div class="form-row">
    <div class="form-group col">
        <label for="quotation">Quotation Number</label>
        <input type="text" placeholder = "0" id="quotation" name="quotation" class="form-control">
    </div>
    <div class="form-group col">
        <label for="quotation">Select a File</label>
        <div class="custom-file">
            <input type="file" class="custom-file-input" id="upload" name="upload" accept="application/pdf, application/pdf">
            <label for="upload" class="custom-file-label">No File Choosen</label>
        </div>
    </div>
</div>
    