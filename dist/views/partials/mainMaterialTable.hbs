<div class="col-md-12">
    <div class="table-buttons" id="table-buttons">
        <button class="btn btn-outline-dark" id="create-new" onclick="test()"><i class="fas fa-plus"></i>Add Material</button>
    </div>

<table id="filterable-table" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th scope="col">S.N.</th>
            <th scope="col">MR#</th>
            <th scope="col">Material</th>
            <th scope="col">Supplier</th>
            <th scope="col">Quantatiy</th>
            <th scope="col">Cost</th>
        </tr>
    </thead>
    <tbody>
        {{#each mainConsumable}}
            <tr>
                <th>{{counter @index}}</th>
                <td><a>{{this.consumable.quotationNumber}}</a></td>
                <td><a>{{this.type.consumable_type}}</a></td>
                <td><a>{{this.consumable.supplierName}}</a></td>
                <td><a>{{this.type.consumable_quantity}}</a></td>
                <td><a>{{this.consumable.singleCost}}</a></td>   
            </tr>
        {{/each}}
    </tbody>
</table>
</div>