<div class="col-md-12">
    <table id="user-table" class="table table-bordered table-striped">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">E-mail</th>
                <th scope="col">Profile</th>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
                <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
                <tr>
                    <th>{{this.studentId}}</th>
                    <td>{{this.email}}</td>
                    <td>{{this.type}}</td>
                    <td>{{this.firstName}}</td>
                    <td>{{this.lastName}}</td>
                    <td><a href="/users/display-user/{{this._id}}">Details</a></td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>